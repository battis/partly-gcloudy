{
  "name": "@battis/partly-gcloudy",
  "version": "1.3.0",
  "description": "Idiosyncratic collection of interactions with `gcloud` CLI tool",
  "license": "GPL-3.0",
  "author": {
    "name": "Seth Battis",
    "url": "https://github.com/battis"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "del ./dist ./tmp",
    "build": "run-s build:*",
    "build:clean": "run-s clean",
    "build:scripts": "tsc -p ./tsconfig.scripts.json",
    "build:google-services": "op run --env-file ./.env -- node ./dist/scripts/services.js -- -v",
    "build:compile": "tsc",
    "release": "run-s release:*",
    "release:build": "run-s build",
    "release:commit-and-tag-version": "commit-and-tag-version",
    "release:publish": "pnpm publish --otp $(op item get npm --otp) --access=public"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.0",
    "lodash": "^4.17.21",
    "ora": "^8.2.0",
    "rword": "^4.0.0",
    "rword-english-extended": "^1.0.0"
  },
  "devDependencies": {
    "@battis/descriptive-types": "^0.2.3",
    "@battis/eslint-config": "^2.0.5",
    "@battis/prettier-config": "^1.6.4",
    "@battis/typescript-tricks": "^0.7.4",
    "@qui-cli/colors": "^3.0.1",
    "@qui-cli/core": "^4.0.1",
    "@qui-cli/env": "^4.0.1",
    "@qui-cli/log": "^3.0.1",
    "@qui-cli/plugin": "^3.0.0",
    "@qui-cli/root": "^3.0.1",
    "@qui-cli/shell": "^3.0.1",
    "@qui-cli/validators": "^3.0.1",
    "@tsconfig/node20": "^20.1.6",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.2.0",
    "commit-and-tag-version": "^12.5.2",
    "del-cli": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.5.3",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "@qui-cli/colors": "3.x",
    "@qui-cli/core": "4.x",
    "@qui-cli/env": "4.x",
    "@qui-cli/log": "3.x",
    "@qui-cli/plugin": "3.x",
    "@qui-cli/root": "3.x",
    "@qui-cli/shell": "3.x",
    "@qui-cli/validators": "3.x"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=12.17"
  },
  "target": "node"
}
